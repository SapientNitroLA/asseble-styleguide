<!--
/**
 *
 * @title Default comment block partial
 *
 */
-->

<section id="{{toID title}}" class="sugar-section-default">

  <h2>{{ title }}</h2>

  {{#each files as |file| }}

    {{#each file.data as |data| }}

      {{> block-title data=data}}

      {{> preview file=file}}

      {{> tag-details file=file}}

    {{/each}}

    {{> file-path file=file}}

  {{/each}}

</section>